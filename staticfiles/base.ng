h1 {
  font-family: Lato;
  margin: 0 auto;
  transform: translateX(-10%);
  display: table;
  margin-top: 15px;
  margin-bottom: 15px;
}

h2 {
  font-family: Lato;
  margin: 0 auto;
  transform: translateX(-10%);
  display: table;
  margin-top: 15px;
  margin-bottom: 15px;
}

body > div > div > h1 {
  margin-top: 25px;
}

h3 {
  font-family: Lato;
}

p {
  font-family: Merriweather;
  font-size: 1.2rem;
  text-align: right;
}

body > div > form > div > table > tbody > tr {
  margin-bottom: 5px;
  font-family: Lato;
  font-size: 1.1rem;
}

body > div > form > table {
  margin: 0 auto;
  display: table;
}

.meal-columns {
  display: flex;
  gap: 20px;
}

.meal-column {
  flex: 1;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 5px;
  background-color: #f9f9f9;
}

.meal-column h3 {
  text-align: center;
  margin-bottom: 10px;
}

.meal-column ul {
  list-style-type: none;
  padding: 0;
}

.meal-column li {
  margin-bottom: 8px;
}

.element-spacer {
  display: grid;
  grid-template-columns: 45vw 45vw;
  grid-template-rows: 20vh 20vh 20vh;
  gap: 10px;
  margin: 6px;
  max-width: 800px;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.leaf {
  grid-column: 2 / 2;
  grid-row: 1 / 2;
  transform: scale(0.8);
  z-index: 100;
}

.myplan-list-title2 {
  margin-top: 25px;
  text-align: center;
  margin-bottom: 30px;
}

.element-container2 {
  display: flex;
  justify-content: center;
}

.form-table2 {
  border-collapse: collapse;
  margin: 0 auto;
}

.form-row {
  margin-bottom: 25px;
}

.form-row td {
  padding: 10px;
}

.margin-btm-30 {
  margin-bottom: 15px;
}

.myplan-list-container-main {
  display: flex;
  flex-wrap: wrap; /* Allows wrapping to a new row */
  justify-content: flex-start; /* Align items to the left */
  gap: 10px; /* Adds spacing between items */
  max-width: 1000px; /* Restricts the container width */
  margin: 0 auto; /* Centers the container */
}


.myplan-list-row-main {
  flex: 1 1 calc(33.33% - 10px); /* Adjusts width to fit 3 items per row with gaps */
  max-width: 250px; /* Ensures items don't exceed this width */
  box-sizing: border-box; /* Includes padding and border in width/height */
}

.myplan-block-main {
  flex-grow: 0;
  margin-bottom: 2px;
}
.myplan-actions-main {
  display: flex;
  justify-content: space-between;
  margin-top: auto;
}
.meal-form-wrap-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
}
.meal-form-wrap {
  display: grid;
  grid-template-columns: 250pz 250px 250px 250px;
  grid-template-rows: 10vh 20vh 10vh;
  color: #333333;
  border: 2pz solid goldenrod;
  border-radius: 10px;
  gap: 5px;
  margin: 6px;
  max-width: 800px;
}

.meal-form-item1 {
  grid-column: 1 / 2;
  grid-row: 1 / 2;
}

.meal-form-item2 {
  grid-column: 1 / 2;
  grid-row: 2 / 3;
}

.meal-form-item3 {
  grid-column: 2 / 3;
  grid-row: 1 / 2;
}

.meal-form-item4 {
  grid-column: 3 / 4;
  grid-row: 3 / 4;
}
