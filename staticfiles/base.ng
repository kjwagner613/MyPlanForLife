.root {
  max-width: 1000px;
  width: 100%;
  margin: 0 auto;
  overflow-x: hidden;
}

body {
  background-color: #f2e9d9;
  color: #333333;
}

.myplan-list-container {
  display: grid;
  grid-template-columns: 23vw 23vw 23vw;
  grid-template-rows: 10vh 10vh 10vh;
  gap: 10px;
  margin: 6px;
  border: 1px solid white;
}

.myplan-list-item {
  border: 3px solid black;
}
.body > div > h1 {
  z-index: 800;
  margin-top: -24 vh;
}

.myplan-actions > a.btn.warn {
  background-color: #e8d5b7;
  color: #333333;
  border: 8px solid #558c65;
  padding: 5px 16px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
  transition: background-color 0.3s ease, transform 0.2s ease, padding 0.3s ease,
    border 0.3s ease;
  box-sizing: border-box;
}

.myplan-actions > a.btn.warn:hover {
  background-color: #827365;
  color: #333333;
  transform: scale(1.05);
  padding: 8px 18px;
  border: 10px solid #a63d08;
}

.myplan-actions > a.btn.warn:focus {
  outline: 2px solid #b4450a;
  outline-offset: 2px;
  background-color: #a63d08;
  color: red;
  padding: 10px 20px;
  border: 12px solid #b4450a;
}

.myplan-actions > a.btn.danger {
  background-color: #827365;
  color: white;
  border: 8px solid rgb(198, 119, 119);
  padding: 5px 10px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
  transition: background-color 0.3s ease, transform 0.2s ease, padding 0.3s ease;
}

.myplan-actions > a.btn.danger:hover {
  background-color: rgb(216, 68, 68);
  color: white;
  font-size: 22px;
  transform: scale(1.05);
  padding: 8px 12px;
}

.myplan-actions > a.btn.danger:focus {
  outline: 2px solid #b4450a;
  outline-offset: 2px;
  background-color: #a63d08;
  color: red;
  padding: 10px 15px;
}

body > div > h1 {
  text-align: center;
  font-size: 2.5em;
  margin: 20px 0;
  color: white;
  text-shadow: 4px 2px 4px #333333;
  font-family: "Arial", sans-serif;
  font-weight: bold;
  text-transform: uppercase;
  letter-spacing: 2px;
  text-decoration: underline;
}

.myplan-list-title {
  font-family: Playfair Display;
}

.body-div-item {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(200px, 2fr));
  grid-template-rows: repeat(auto-fill, minmax(200px, 2fr));
  gap: 10px;
  margin: 6px;
  border: 1px solid white;
}

button {
  background-color: #e8d5b7;
  color: #333333;
  border: 4px solid #558c65;
  border-radius: 10px;
  padding: 4px 6px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
  transition: background-color 0.3s ease, transform 0.2s ease, padding 0.3s ease;
}

button:hover {
  background-color: #827365;
  color: #333333;
  transform: scale(1.3);
  padding: 6px 8px;
}

button:focus {
  outline: 2px solid #b4450a;
  outline-offset: 2px;
  background-color: #a63d08;
  color: red;
  padding: 10px 16px;
}

